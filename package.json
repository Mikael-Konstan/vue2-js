    "vue": "2.6.14",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "element-ui": "^2.13.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.14"
